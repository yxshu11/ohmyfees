<% provide(:title, @utility_fee.name) %>

<div class="container">
<h1>Utility Fee Information:</h1>
  <div class="row">
    <div class="col-md-6 col-md-offset-3 well">
			<h4>
			<table class="table noborder">
        <tr>
          <td class="no-repeat-header"><b>Name:</b></td>
          <td><%= @utility_fee.name %></td>
				</tr>
				<tr>
          <td><b>Amount:</b></td>
					<td>RM&nbsp;<%= @utility_fee.amount %></td>
				</tr>
				<tr>
          <td><b>Description:</b></td>
					<td><%= @utility_fee.description %></td>
        </tr>
				<tr>
					<td></td>
					<td></td>
				</tr>
			</table>
			</h4>
				<section class="center">
				<%= link_to "Edit", edit_utility_fee_path(@utility_fee), class: "btn btn-primary" %>  &nbsp;
				<%= link_to "Back", utility_fees_path, class: "btn btn-default" %>
			</section>
		</div>
	</div>
</div>
